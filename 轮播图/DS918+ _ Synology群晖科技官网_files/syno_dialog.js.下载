SYNO_WEB.alert=function(n,o,a){var i;(i=n instanceof jQuery?n.clone():$(n)).is(".dialog")||(i=$("<div class='dialog'>").append(i));var e={items:{src:i,type:"inline"}};"function"==typeof o&&(e.callbacks={close:o}),a&&(e.mainClass=a),"syno_language_dialog"===a&&(e.mainClass="syno_language_dialog syno_mfp_content_transparent syno_mfp_zoom_out",e.closeMarkup='<div class="syno_mfp_close syno_mfp_close_language_dialog"><img src="img/footer/close_icon.png" srcset="img/footer/close_icon@2x.png 2x" alt=""></div>',e.removalDelay=200),$.magnificPopup.open(e);var c=$(".syno_mfp_close");c.length&&c.each(function(){$(this).on("click",function(n){n.preventDefault(),$.magnificPopup.close()})}),$.magnificPopup.instance.isOpen&&$(".mfp-container").on("click","a",function(){$(this).attr("href")&&$.magnificPopup.close()})};