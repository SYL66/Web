var SYNO_MENU=SYNO_MENU||{};!function(){function e(){i.find(".active").removeClass("active"),$("#menuitems").removeClass("active"),$("body").is(".desktop")?$(".js-vertical-tab-content").hide():$(".js-vertical-tab-content").removeAttr("style"),$("#menuitems").find(".synomenu").removeAttr("style"),a.removeClass("open"),$("body").removeClass("menu_open"),$(".layer_2").slideUp(),i.find(".rotate").removeClass("rotate"),t.hide()}var i=$("#main_menu"),t=$("#mask"),a=$("#menu_link");SYNO_MENU.init=function(){var n;i.on("click",".has_child",function(e){e.preventDefault(),e.stopPropagation(),$("#menuitems").find(".synomenu").removeAttr("style")}).on("mouseenter",".synomenu.d1 > li > a",function(e){var a=this;$("body").is(".desktop")&&(n||(n=window.setTimeout(function(){if($(a).is(".has_child")){$(".vertical-tabs a").removeClass("is-active"),$(".vertical-tab-content-container > div").hide(),n=null;var e=$(a).parent("li").addClass("active");i.find(".active").not(e).removeClass("active"),e.find(".vertical-tabs a:first-child").addClass("is-active"),e.find(".vertical-tab-content-container div:first-child").show(),$(a).parent("li").index(),t.show()}else i.find(".active").removeClass("active"),i.find("is-active").removeClass("is-active")},100)))}).on("mouseleave",".synomenu.d1 > li > a",function(e){n&&(window.clearTimeout(n),n=null)}).on("mouseleave","#menuitems",function(e){$("body").is(".desktop")&&(i.find(".active").removeClass("active"),i.find("is-active").removeClass("is-active"),t.hide())}).on("mouseenter",".brand",function(e){e.stopPropagation(),$("body").is(".desktop")&&(i.find(".active").removeClass("active"),i.find("is-active").removeClass("is-active"),t.hide())}).on("click",".layer_1 > ul> li:not('.has_child'), .layer_2 li",function(e){e.preventDefault(),e.stopPropagation();var i=$(this).data("layer_active");if($(".layer_3_fixed, ."+i).addClass("active"),$(this).data("layer3_show")){var t=$(this).data("layer3_show"),a=$(this).data("layer_active");$(".layer_3_fixed li."+a).html($(this).html()),$(".mobile_menu li .vertical-tab-content:not(#tab_"+t+")").hide()}}).on("click",".layer_1 > ul > li.has_child span",function(e){$(this).toggleClass("rotate"),$(this).next("ul").slideToggle()}).on("click",".layer_3_fixed",function(e){$('.layer_3_fixed, .layer_3_fixed li, .mobile_menu li:not(".layer_1")').removeClass("active"),$(".mobile_menu li .vertical-tab-content").show(),$(".mobile_menu li").scrollTop(0)}).on("click",".mobile_menu li .js-vertical-tab-content a",function(i){$("#menuitems").find(".active").removeClass("active"),a.removeClass("open"),e()}),a.on("click",function(t){$(".mobile_menu").is(".active")?e():($("body").addClass("menu_open"),$(".mobile_menu, .layer_1, .mobile_menu_top").addClass("active"),i.find(".vertical-tab-content").show(),a.addClass("open"))}),SYNO_HELPER.on("resize",{key:"SYNO_MENU",element:"#main_menu",trigger:!0},function(){t.hide(),$(window).outerWidth()>=1024&&e()}),i.on("click","li > a:not(.has_child)",e),$(".js-vertical-tab-content.show").show();var o;$(".js-vertical-tab").on("mouseenter",function(e){var i=this;e.preventDefault(),e.stopPropagation(),o||(o=window.setTimeout(function(){var e,t=$(i).closest(".vertical-tabs-container");o=null,t.find(".js-vertical-tab-content").hide(),e=$(i).attr("rel"),$("#"+e).show(),t.find(".js-vertical-tab").removeClass("is-active"),$(i).addClass("is-active")},80))}).on("mouseleave",function(e){o&&(window.clearTimeout(o),o=null)})}}();